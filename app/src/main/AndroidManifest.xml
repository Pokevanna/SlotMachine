<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.johw.giovanna">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.johw.giovanna.SplashScreen"
            android:theme="@style/FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.johw.giovanna.MenuActivity" android:theme="@style/FullScreen"/>
        <activity android:name="com.example.johw.giovanna.MainMenuActivity" android:theme="@style/FullScreen"/>
        <activity android:name="com.example.johw.giovanna.SobreActivity" android:theme="@style/FullScreen"/>
        <activity android:name="com.example.johw.giovanna.SlotMachineActivity" android:theme="@style/FullScreen"/>
    </application>

</manifest>